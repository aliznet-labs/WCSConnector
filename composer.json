{
    "name": "Aliznet/WCSBundle",
    "type": "symfony-bundle",
    "description": "The Websphere Commerce Connector for Akeneo",
    "keywords": ["wcs", "akeneo", "pim"],
    "license": "GPL-3.0+",
    "authors": [
        {
            "name": "Aliznet",
            "homepage": "http://www.aliznet.fr"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "weaverryan/derp-dangerzone",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/aliznet-labs/WCSConnector.git",
                    "type": "git",
                    "reference": "master"
                },
                "autoload": {
                    "psr-0" : {
                        "Aliznet" : "src"
                    }
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": ">=2.1", 
        "twig/twig": "*",
        "doctrine/doctrine-bundle": "*"
    }
}